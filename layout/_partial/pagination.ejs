<ul class="pager">
    <% if (page.prev){ %>
        <li class="previous"><a href="<%- config.root %><%- page.prev_link %>">&larr;  <%- __('Newer Posts')%></a></li>
    <% } %>
    <% if (page.next){ %>
        <li class="next"><a href="<%- config.root %><%- page.next_link %>"><%- __('Previous Posts')%>  &rarr;</a></li>
    <% } %>
</ul>
